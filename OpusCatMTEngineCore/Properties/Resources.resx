  <data name="StanzaWrapperCode" xml:space="preserve">
    <value>import stanza</value>
  </data>
  <data name="String" xml:space="preserve">
    <value>class StanzaWrapper:</value>
  </data>
  <data name="String1" xml:space="preserve">
    <value>def __init__(self,</value>
  </data>
  <data name="String10" xml:space="preserve">
    <value>return lemma_tuples</value>
  </data>
  <data name="String2" xml:space="preserve">
    <value>lang,</value>
  </data>
  <data name="String3" xml:space="preserve">
    <value>processors):</value>
  </data>
  <data name="String4" xml:space="preserve">
    <value>self.stanza = stanza.Pipeline(lang, processors='tokenize,pos,lemma,depparse')</value>
  </data>
  <data name="String5" xml:space="preserve">
    <value>def lemmatize(self,input):</value>
  </data>
  <data name="String6" xml:space="preserve">
    <value>doc = self.stanza(input)</value>
  </data>
  <data name="String7" xml:space="preserve">
    <value>lemma_tuples = []</value>
  </data>
  <data name="String8" xml:space="preserve">
    <value>for sentence in doc.sentences:</value>
  </data>
  <data name="String9" xml:space="preserve">
    <value>lemma_tuples +=[(x.start_char,x.end_char,x.lemma) for x in sentence.words]</value>
  </data>
